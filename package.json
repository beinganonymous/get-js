{
  "name": "get-js",
  "version": "0.0.5",
  "description": "A lightweight promise based package to load scripts on the fly",
  "unpkg": "dist/",
  "jsdelivr": "dist/",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "randomatic": ">=3.0.0",
    "webpack": "^4.40.2",
