{
  "name": "get-js",
  "version": "0.0.1-0",
  "description": "A Promise based package to load scripts dynamically",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "webpack": "^1.13.0"
  },
  "scripts": {
    "transpile": "babel src -d lib",
    "bundle": "webpack -d --output-filename get.js && webpack -p --output-filename get.min.js",
    "build": "npm run transpile && npm run bundle",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kabirbaidhya/get-js.git"
  },
  "author": "Kabir Baidhya <kabirbaidhya@gmail.com>",
  "keywords": [
    "javascript",
    "dynamically",
    "load-script",
    "get-script",
    "load-js",
    "promise",
    "get-js",
    "async",
    "get",
    "es6"
  ],
  "files": [
    "lib/",
    "index.js",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "license": "MIT"
}
